{
  "name": "@percy/protractor",
  "version": "1.1.0",
  "description": "Percy.io integration with Protractor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/percy/percy-protractor.git"
  },
  "keywords": [
    "protractor",
    "testing"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Perceptual Inc.",
  "license": "MIT",
  "devDependencies": {
    "@percy/tslint": "^1.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "chromedriver": "^83.0.0",
    "http-server": "^0.12.0",
    "protractor": "^5.4.1",
    "semantic-release": "^17.0.4",
    "typescript": "^3.1.6"
  },
  "resolutions": {
    "ecstatic": "3.2.2"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "webdriver-update": "webdriver-manager update --gecko false --standalone false",
    "install": "npm run webdriver-update || npm run webdriver-update",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "percy exec -- protractor conf.js"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@percy/agent": "~0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
